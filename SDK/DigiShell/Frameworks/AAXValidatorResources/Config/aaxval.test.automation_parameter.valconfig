process {
	id: "process_automation_parameter_cpp"
	name: "Automation Parameter Name Test CPP"
	arguments {
		elem: "--bundle_path=\"$AAXVAL_PARAM_AAXPLUGIN$\""
		elem: "--uniq_id=$AAXVAL_PARAM_UNIQ_ID$"
		elem: "--port=$AAXVAL_TCP_PORT$"
    }
	platform_process {
		platform: E_PLATFORM_OSX
		invocation: "$AAXVAL_DIR_TOOLS$/CommandLineTools/aaxval_test_automation_parameter"
	}
	platform_process {
		platform: E_PLATFORM_WIN
		invocation: "$AAXVAL_DIR_TOOLS$\\aaxval.test.automation_parameter.exe"
	}
}

test {
	id: "test.page_table.automation_list"
	name: "Plug-In Page Table Automation Parameter Test"
	description: "Plug-in validator module to test the parameter automation list in a plug-in's page tables"
	
	condition {
		id: "condition.no_page_table_exists"
		type: E_CONDITIONTYPE_PASS
		description: "No page table exists"
	}
	condition {
		id: "condition.duplicate_long_names"
		type: E_CONDITIONTYPE_WARN
		description: "There are duplicate 31-character names"
	}
	condition {
		id: "condition.incomplete_default_table"
		type: E_CONDITIONTYPE_WARN
		description: "There are automatable parameters not included in the default one-control-per-page table"
	}
	condition {
		id: "condition.failed_to_get_control_names_from_page_table"
		type: E_CONDITIONTYPE_FAIL
		description: "Failed to get control names from the page table"
	}
	condition {
		id: "condition.duplicate_ids"
		type: E_CONDITIONTYPE_FAIL
		description: "Found duplicate parameter IDs"
	}
	condition {
		id: "condition.invalid_id_in_page_table"
		type: E_CONDITIONTYPE_FAIL
		description: "A parameter ID used in the page table does not match either a parameter ID or a 31-length name"
	}
	condition {
		id: "condition.default_table_contains_duplicate_entries"
		type: E_CONDITIONTYPE_FAIL
		description: "The default one-control-per-page table contains duplicate entries"
	}
	condition {
		id: "condition.default_table_contains_duplicate_name_id_reference"
		type: E_CONDITIONTYPE_FAIL
		description: "The default one-control-per-page table contains separate entries for the same parameter's name and ID"
	}
	
	module_id: "mod_tests_plat_any"
	module_id: "mod_tests_fast"
	category_id: "cat_reliability"
	
	process_id: "process_automation_parameter_cpp"
}
