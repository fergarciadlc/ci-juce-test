#Maximum number of log files
logfilelimit=10
#Is append to old log file?
Append=false
#path to logs
FileTracingDir=/opt/log/zeta-app
#'Power on' counter, stored in file
counterfile=/opt/usr/digitrace/counter.txt

# Enable error-level logging for all facilities
DTF_ALL=file@DTP_NORMAL

# Some specific facilities that might be useful for debugging
# Uncomment these lines to turn on more verbose logging
# Add ",console@DTP_LOW" for console logging

DTF_DEVICE_APP=file@DTP_LOWEST
DTF_DSI=file@DTP_LOWEST
#DTF_REMOTE_CONTROL=file@DTP_LOW