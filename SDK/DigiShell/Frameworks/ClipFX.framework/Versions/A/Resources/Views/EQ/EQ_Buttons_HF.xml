<?xml version="1.0" encoding="UTF-8"?>

<view type="View"  id="EQ Buttons HF View">
    <bounds x="0" y="49" width="245" height="21"/>
    <intercepts_mouse value="false"/>
    
    <view type="Button" id="EQ High Band In/Out">
        <parameter id="EQ High In/Out"/>
        <bounds x="0" y="1" width="32" height="15"/>
        <image_list id="state">
            <image id="" src="Buttons/btn_eq_HF_off.png"/>
            <image id="" src="Buttons/btn_eq_HF_on.png"/>
        </image_list>
        <toggles value="1"/>
	 	<highlight thickness="1">
	    	<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    </highlight>
  	</view>
    
    <view type="Button" id="EQ High Band Type Button">
        <parameter id="EQ High Type"/>
        <bounds x="42" y="0" width="44" height="17"/>
        <image_list id="state">
            <image id="" src="Buttons/btn_eq_hf_band_pass_on.png"/>
            <image id="" src="Buttons/btn_eq_hf_high_shelf_on.png"/>
        </image_list>
        <segments range="2"/>
	 	<highlight thickness="1">
	    	<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    </highlight>
   </view>
    
    <view type="View"  id="EQ Edit 1 View">
        <bounds x="101" y="2" width="160" height="100"/>
        <intercepts_mouse value="false"/>
        
        <view type="EditText" id="EQ High Band Freq Text">
            <parameter id="EQ High Frequency"/>
            <bounds x="0" y="0" width="50" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <color id="text" red="198" green="198" blue="198" alpha="255"/>
            <inset top="2" left="2" bottom="2" right="2"/>
	 		<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	   		</highlight>
        </view>
        
        <view type="EditText" id="EQ High Band Gain Text">
            <parameter id="EQ High Gain"/>
            <tracking fine="481"/>
            <bounds x="55" y="0" width="45" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <color id="text" red="198" green="198" blue="198" alpha="255"/>
            <inset top="2" left="2" bottom="2" right="2"/>
	 		<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	   		</highlight>
        </view>
        
        <view type="EditText" id="EQ High Band Q Text">
            <parameter id="EQ High Q"/>
            <bounds x="105" y="0" width="38" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <layout_if parameter="EQ High Type" operator="!=" string="High-Shelf">
                <color id="text" red="198" green="198" blue="198" alpha="255"/>
            </layout_if>
            <layout_if parameter="EQ High Type" operator="=" string="High-Shelf">
                <color id="text" red="91" green="95" blue="98" alpha="255"/>
            </layout_if>
            <inset top="2" left="2" bottom="2" right="2"/>
	 		<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	   		</highlight>
         </view>
        
    </view>
    
</view>
