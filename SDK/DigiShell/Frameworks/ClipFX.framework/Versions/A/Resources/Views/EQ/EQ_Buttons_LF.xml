<?xml version="1.0" encoding="UTF-8"?>

<view type="View"  id="EQ Buttons LF View">
    <bounds x="0" y="143" width="245" height="21"/>
    <intercepts_mouse value="false"/>
    
    <view type="Button" id="EQ Low Band In/Out">
        <parameter id="EQ Low In/Out"/>
        <bounds x="0" y="1" width="32" height="15"/>
        <image_list id="state">
            <image id="" src="Buttons/btn_eq_LF_off.png"/>
            <image id="" src="Buttons/btn_eq_LF_on.png"/>
        </image_list>
        <toggles value="1"/>
	 	<highlight thickness="1">
	    	<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    </highlight>
   </view>
    
    <view type="Button" id="EQ Low Band Type Button">
        <parameter id="EQ Low Type"/>
        <bounds x="42" y="0" width="44" height="17"/>
        <image_list id="state">
            <image id="" src="Buttons/btn_eq_lf_band_pass_on.png"/>
            <image id="" src="Buttons/btn_eq_hf_low_shelf_on.png"/>
        </image_list>
        <segments range="2"/>
	    <highlight thickness="1">
	    	<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    </highlight>
    </view>
    
    <view type="View"  id="EQ Edit 1 View">
        <bounds x="101" y="2" width="160" height="100"/>
        <intercepts_mouse value="false"/>
        
        <view type="EditText" id="EQ Low Band Freq Text">
            <parameter id="EQ Low Frequency"/>
            <bounds x="0" y="0" width="50" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <color id="text" red="198" green="198" blue="198" alpha="255"/>
            <inset top="2" left="2" bottom="2" right="2"/>
	    	<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    	</highlight>
        </view>
        
        <view type="EditText" id="EQ Low Band Gain Text">
            <parameter id="EQ Low Gain"/>
            <tracking fine="481"/>
            <bounds x="55" y="0" width="45" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <color id="text" red="198" green="198" blue="198" alpha="255"/>
            <inset top="2" left="2" bottom="2" right="2"/>
	    	<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    	</highlight>
        </view>
        
        <view type="EditText" id="EQ Low Band Q Text">
            <parameter id="EQ Low Q"/>
            <bounds x="105" y="0" width="38" height="17"/>
            <font typeface="Arial" height="11" bold="0" italic="0" underlined="0"/>
            <justification horizontal="left" vertical="center"/>
            <layout_if parameter="EQ Low Type" operator="!=" string="Low-Shelf">
                <color id="text" red="198" green="198" blue="198" alpha="255"/>
            </layout_if>
            <layout_if parameter="EQ Low Type" operator="=" string="Low-Shelf">
                <color id="text" red="91" green="95" blue="98" alpha="255"/>
            </layout_if>
            <inset top="2" left="2" bottom="2" right="2"/>
	    	<highlight thickness="1">
	    		<color id="blue" red="0" green="100" blue="255" alpha="255"/> 
	    	</highlight>
       </view>
        
    </view>
    
</view>