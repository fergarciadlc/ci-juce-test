<?xml version="1.0" encoding="UTF-8"?>

<view type="DragAndDropView" id="InactiveStateView">
	<bounds x="0" y="0" width="1482" height="188"/>
	<intercepts_mouse value="false"/>

	<!-- Front panel -->
	<view type="View" id="InactiveFrontPanel">
		<bounds x="0" y="0" width="69" height="188"/>
		<image_list id="background" src="inactive_front_panel.png"/>
		<intercepts_mouse value="false"/>
	</view>

	<!-- EQ Section -->
	<view type="Reveal" id="Inactive EQ Section">
		<reveal id="Inactive Reveal EQ Button" width="22" height="188"/>
		<parameter id="EQ Reveal"/>
		<bounds x="74" y="0" width="560" height="188"/>
		<image id="open" src="inactive_eq.png"/>
		<image id="closed" src="inactive_eq_collapsed.png"/>
		<intercepts_mouse value="false"/>
		
		<view type="View" id="Header">
			<layout_if parameter="EQ Reveal" operator="=" value="0">
				<bounds x="0" y="0" width="560" height="50"/>
			</layout_if>
			<layout_if parameter="EQ Reveal" operator="=" value="1">
				<bounds x="0" y="0" width="22" height="188"/>
			</layout_if>
			<intercepts_mouse value="false"/>
		</view>
		
		<view type="Button" id="Inactive Reveal EQ Button">
			<parameter id="EQ Reveal"/>
			<layout_if parameter="EQ Reveal" operator="=" value="0">
				<bounds x="544" y="172" width="16" height="15"/>
			</layout_if>
			<layout_if parameter="EQ Reveal" operator="=" value="1">
				<bounds x="6" y="172" width="16" height="15"/>
			</layout_if>
			<image_list id="state">
				<image id="" src="Buttons/show_pane_locations_hide.png"/>
				<image id="" src="Buttons/show_pane_locations.png"/>
			</image_list>
			<toggles value="true"/>
		</view>
	</view>

	<!-- Filters Section -->
	<view type="Reveal" id="Inactive Filters Section">
		<reveal id="Inactive Reveal Filters Button" width="22" height="188"/>
		<parameter id="Filters Reveal"/>
		<bounds x="639" y="0" width="168" height="188"/>
		<image id="open" src="inactive_filters.png"/>
		<image id="closed" src="inactive_filters_collapsed.png"/>	
		<intercepts_mouse value="false"/>
		
		<view type="View" id="Header">
			<layout_if parameter="Filters Reveal" operator="=" value="0">
				<bounds x="0" y="0" width="168" height="50"/>
			</layout_if>
			<layout_if parameter="Filters Reveal" operator="=" value="1">
				<bounds x="0" y="0" width="22" height="188"/>
			</layout_if>
			<intercepts_mouse value="false"/>
		</view>
		
		<view type="Button" id="Inactive Reveal Filters Button">
			<parameter id="Filters Reveal"/>
			<layout_if parameter="Filters Reveal" operator="=" value="0">
				<bounds x="152" y="172" width="16" height="15"/>
			</layout_if>
			<layout_if parameter="Filters Reveal" operator="=" value="1">
				<bounds x="6" y="172" width="16" height="15"/>
			</layout_if>
			<image_list id="state">
				<image id="" src="Buttons/show_pane_locations_hide.png"/>
				<image id="" src="Buttons/show_pane_locations.png"/>
			</image_list>
			<toggles value="true"/>
		</view>
	</view>

	<!-- Dynamics Section -->
	<view type="Reveal" id="Inactive Dynamics Section">
		<reveal id="Inactive Reveal Dynamics Button" width="22" height="188"/>
		<parameter id="Dynamics Reveal"/>
		<bounds x="812" y="0" width="614" height="188"/>
		<image id="open" src="inactive_dynamics.png"/>
		<image id="closed" src="inactive_dynamics_collapsed.png"/>	
		<intercepts_mouse value="false"/>
		
		<view type="View" id="Header">
			<layout_if parameter="Dynamics Reveal" operator="=" value="0">
				<bounds x="0" y="0" width="614" height="50"/>
			</layout_if>
			<layout_if parameter="Dynamics Reveal" operator="=" value="1">
				<bounds x="0" y="0" width="22" height="188"/>
			</layout_if>
			<intercepts_mouse value="false"/>
		</view>
		
		<view type="Button" id="Inactive Reveal Dynamics Button">
			<parameter id="Dynamics Reveal"/>
			<layout_if parameter="Dynamics Reveal" operator="=" value="0">
				<bounds x="597" y="172" width="16" height="15"/>
			</layout_if>
			<layout_if parameter="Dynamics Reveal" operator="=" value="1">
				<bounds x="6" y="172" width="16" height="15"/>
			</layout_if>
			<image_list id="state">
				<image id="" src="Buttons/show_pane_locations_hide.png"/>
				<image id="" src="Buttons/show_pane_locations.png"/>
			</image_list>
			<toggles value="true"/>
		</view>
	</view>

</view>
